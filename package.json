{
  "name": "stencil-todo",
  "private": true,
  "version": "0.0.1",
  "description": "stencil-todo",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format.check": "prettier --config .prettierrc --check 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@capacitor/core": "^1.3.0",
    "@ionic/core": "^4.11.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^1.3.0",
    "@stencil/core": "^1.8.1",
    "@types/jest": "24.0.23",
    "@types/puppeteer": "1.20.3",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "puppeteer": "1.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
